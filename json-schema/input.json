{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Model input",
  "description": "Describe the required model input.",
  "type": "object",
  "properties": {
    "input": {
      "type": "object",
      "required": [
        "name",
        "shape:batch",
        "shape:channels",
        "shape:height",
        "shape:width"
      ],
      "properties": {
			"name": {
			  "title": "name",
			  "description": "name of the input varaible",
			  "type": "string"
			},
			"shape:batch": {
			  "title": "batch size",
			  "description": "tensor batch size",
			  "type": "integer",
			  "minimum": 1
			},
			"shape:channels": {
			  "title": "number of channels",
			  "description": "tensor channel dimension",
			  "type": "integer",
			  "minimum": 1
			},
			"shape:height": {
			  "title": "image height",
			  "description": "tensor image height",
			  "type": "integer",
			  "minimum": 1
			},
			"shape:width": {
			  "title": "image width",
			  "description": "tensor image width",
			  "type": "integer",
			  "minimum": 1
			}
		}
    }
  }
}